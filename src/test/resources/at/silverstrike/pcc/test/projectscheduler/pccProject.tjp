project "Sample project" 2011-09-04 +1m 
{
  timeformat "%d.%m.%Y %H:%M"

  numberformat "-" "" "." "," 2
  currencyformat "(" ")" "." "," 0

  now 2011-09-04

  currency "EUR"

  timingresolution 10 min
  
  scenario plan "Plan" {
  }
}

copyright "DP"

resource R57 "DP"
{
	limits { dailymax 8.00h }
}

task T2 "D: Doll"
{
	
start ${now}

	priority 998
	
allocate R57 {limits {dailymax 8.00h}}

	
effort 2.00h

	
	
}

task T3 "T: Train"
{
	
start ${now}

	priority 999
	
allocate R57 {limits {dailymax 8.00h}}

	
effort 3.00h

	
	
}

taskreport status "pccDeadlines" {
 formats csv
 columns id, end
}

export "pccBookings.tji" {
  definitions *
  taskattributes *
  hideresource 0
}
